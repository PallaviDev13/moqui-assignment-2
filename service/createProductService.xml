<service verb="create" service-name="createProduct" package-name="moqui.training">
    <in-parameters>
        <parameter name="productName" type="text-short" required="true"/>
        <parameter name="color" type="text"/>
        <parameter name="size" type="text"/>
    </in-parameters>
    <out-parameters>
        <parameter name="productId" type="id-long"/>
    </out-parameters>

</service>
